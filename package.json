{
  "name": "qcm.generator",
  "version": "1.0.0",
  "description": "qcm generator project",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "start": "webpack-dev-server --open --mode development",
    "build": "webpack --mode production",
    "storybook": "start-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gparquet/qcm.generator.git"
  },
  "author": "geoffrey parquet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gparquet/qcm.generator/issues"
  },
  "homepage": "https://github.com/Gparquet/qcm.generator#readme",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "prettier": "1.17.0",
    "react-test-renderer": "^16.8.6",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/mocks/**",
      "!*.config.js",
      "!**/*.stories.js",
      "!**/index.js",
      "!**/serviceWorker.js",
      "!**/*.constants.js"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 80
      }
    }
  }
}
