{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@axa-fr/react-toolkit-all": "^1.2.5",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "prettier": "prettier \"**/*.{json,md,scss,yaml,yml}\"",
    "start-dev": "concurrently \"react-scripts start\" \"nodemon mocks/index.js\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@storybook/react": "^5.0.11",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "file-system": "^2.2.2",
    "husky": "2.2.0",
    "lint-staged": "8.1.6",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
