		"webpack-cli": "^3.3.2",
		"webpack-dev-server": "^3.3.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm test",
			"pre-push": "npm test"
		}
	},
	"jest": {
		"verbose": true,
		"collectCoverageFrom": [
			"**/*.{js,ts,tsx}",
			"!**/@types/**",
			"!**/node_modules/**",
			"!**/coverage/**",
			"!**/mocks/**",
			"!*.config.ts",
			"!**/*.stories.{ts,tsx}",
			"!**/index.ts",
			"!**/serviceWorker.ts",
			"!**/*.constants.ts"
		],
		"coverageThreshold": {
			"global": {
				"lines": 80
			}
		}
	}
}
